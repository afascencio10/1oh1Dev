<div class="modal fade"
	id="profileCalendarRecurringUnavailableDialogBox"
	tabindex="-1"
	role="dialog"
	aria-labelledby="profileCalendarRecurringUnavailableDialogBox"
	aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			<div class="modal-header px-5">
		        <h6 class="modal-title">Recurring Unavailability</h6>

		        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
		          <span aria-hidden="true">&times;</span>
		        </button>
		    </div>

			<div class="modal-body px-5 position-relative">
				<%= render('profiles/fifth_page',:locals => {:first => 0})%>
		</div>
	</div>
</div>
